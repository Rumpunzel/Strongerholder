[sub_resource type="GDScript" id=2]
script/source = "extends Spatial
tool

export var _turn_speed := 2.0

onready var _cursor: Spatial = $MeshInstance


func _ready() -> void:
	var error := Events.connect(\"clicked_to_move\", _cursor, \"set_translation\")
	assert(error == OK)

func _process(delta: float) -> void:
	_cursor.rotate_y(_turn_speed * delta)
"

